<div class="container">
  <nav aria-label="breadcrumb">
    <ol class="breadcrumb">
      <li class="breadcrumb-item active" aria-current="page">カメラチェック結果一覧</li>
    </ol>
  </nav>
</div>
<div class="container-fluid">
  <div>
    <% if @attendances.any? %>
      <div class="container-fluid">
        <%= paginate @attendances %>
        <div class="table-responsive">
          <table class="table table-striped table-md">
            <tr>
              <th>従業員ID</th>
              <th>名前</th>
              <th>項目1</th>
              <th>項目2</th>
              <th>項目3</th>
              <th>項目4</th>
              <th>項目5</th>
              <th>項目6</th>
              <th>項目7</th>
              <th>項目8</th>
              <th>項目9</th>
              <th>項目10</th>
              <th>点数</th>
              <th>経路</th>
              <th>作成時間</th>
            </tr>
            <% @attendances.each do |attendance| %>
            <tr>
              <td><%= attendance.employee.employee_id %></td>
              <td><%= attendance.employee.name %></td>
              <td><%= attendance.attr_one %></td>
              <td><%= attendance.attr_two %></td>
              <td><%= attendance.attr_three %></td>
              <td><%= attendance.attr_four %></td>
              <td><%= attendance.attr_five %></td>
              <td><%= attendance.attr_six %></td>
              <td><%= attendance.attr_seven %></td>
              <td><%= attendance.attr_eight %></td>
              <td><%= attendance.attr_nine %></td>
              <td><%= attendance.attr_ten %></td>
              <td><%= attendance.point %></td>
              <td><%= attendance.ride_in %></td>
              <td><%= attendance.created_at.strftime('%m/%d-%k:%M') %></td>
            </tr>
            <% end %>
          </table>
        </div>
      </div>
    <% else %>
      <%= "カメラチェックのデータがありません" %>
    <% end %>
  </div>
</div>
